{
  "name": "nextype",
  "version": "0.0.0",
  "description": "Utility tool for NextJS",
  "keywords": [],
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    "client": {
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.cjs.js"
    },
    "server": {
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.cjs.js"
    },
    "shared": {
      "import": "./dist/shared/index.js",
      "require": "./dist/shared/index.cjs.js"
    }
  },
  "main": "./dist/shared/index.cjs.js",
  "module": "./dist/shared/index.js",
  "types": "./dist/shared/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format cjs,esm src/client/index.ts src/server/index.ts src/shared/index.ts --dts src/client/index.ts --dts src/server/index.ts --dts src/shared/index.ts --clean --splitting",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "miid": "^6.0.0",
    "ts-custom-error": "^3.2.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "next": "^12.0.8",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "react-query": "^3.34.8",
    "tslib": "^2.3.1",
    "tsup": "^5.11.11",
    "typescript": "^4.5.4",
    "zenjson": "^1.0.2",
    "zod": "^3.11.6"
  },
  "peerDependencies": {
    "react-query": "^3.34.8",
    "zenjson": "^1.0.2",
    "zod": "^3.11.6"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
