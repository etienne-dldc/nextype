{
  "name": "nextype",
  "version": "1.0.3",
  "description": "Utility tool for NextJS",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/nextype#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/nextype/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/nextype.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    "./client": {
      "import": "./client.mjs",
      "require": "./client.js"
    },
    "./server": {
      "import": "./server.mjs",
      "require": "./server.js"
    },
    "./shared": {
      "import": "./shared.mjs",
      "require": "./shared.js"
    }
  },
  "main": "./shared.js",
  "module": "./shared.mjs",
  "types": "./shared.d.ts",
  "files": [
    "dist",
    "client.d.ts",
    "client.js",
    "client.mjs",
    "server.d.ts",
    "server.js",
    "server.mjs",
    "shared.d.ts",
    "shared.js",
    "shared.mjs"
  ],
  "scripts": {
    "build": "tsup --format cjs,esm src/client.ts src/server.ts src/shared.ts --dts src/client.ts --dts src/server.ts --dts src/shared.ts --clean --splitting",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "miid": "^6.0.0",
    "ts-custom-error": "^3.2.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "next": "^12.0.8",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "react-query": "^3.34.8",
    "tslib": "^2.3.1",
    "tsup": "^5.11.11",
    "typescript": "^4.5.4",
    "zenjson": "^1.0.3",
    "zod": "^3.11.6"
  },
  "peerDependencies": {
    "react-query": "^3.34.8",
    "zenjson": "^1.0.3",
    "zod": "^3.11.6"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
